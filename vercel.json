// {
// "rewrites":[{"source":"/(.*)","destination":"/"}]
// }

{
    "version": 2,
    "builds": [
      {
        "src": "server.js", // Specify your server entry point
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  